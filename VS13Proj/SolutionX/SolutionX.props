<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SXROOT>$(SolutionDir)..\..</SXROOT>
    <SX_INC>$(SXROOT)\include</SX_INC>
    <SX_SRC>$(SXROOT)\src</SX_SRC>
    <SX_LIB>$(SXROOT)\lib</SX_LIB>
    <SX_BIN>$(SXROOT)\bin</SX_BIN>
  </PropertyGroup>
  <PropertyGroup>
    <LibraryPath>$(SXROOT)\lib;$(VC_LibraryPath_x64);$(WindowsSDK_LibraryPath_x64);</LibraryPath>
    <SourcePath>$(SXROOT)\src;$(VC_SourcePath);</SourcePath>
    <IncludePath>$(SXROOT)\src;$(VC_IncludePath);$(WindowsSDK_IncludePath);</IncludePath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="SXROOT">
      <Value>$(SXROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="SX_INC">
      <Value>$(SX_INC)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="SX_SRC">
      <Value>$(SX_SRC)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="SX_LIB">
      <Value>$(SX_LIB)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="SX_BIN">
      <Value>$(SX_BIN)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>